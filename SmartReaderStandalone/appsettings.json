{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }

    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://127.0.0.1:5341" }
      }
    ]
  },

  "ReaderInfo": {
    "DebugAddress": "192.168.68.248",
    "Address": "127.0.0.1"
  },
  "AllowedHosts": "*",
